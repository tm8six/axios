{
  "name": "origami-flex",
  "version": "1.14.0",
  "sprint": 49,
  "scripts": {
    "start": "expo start --dev-client",
    "pod-install": "(cd ios && bundle exec pod install && rm -rf .xcode.env.local)",
    "prebuild": "yarn build:expo-plugins && expo prebuild --clean && rm -rf ios/.xcode.env.local # Clean up the xcode env file",
    "android:dev": "NODE_ENV=development expo run:android",
    "ios:dev": "NODE_ENV=development expo run:ios",
    "ios:dev:clean": "yarn clean && yarn ios:dev",
    "android:dev:clean": "yarn clean; && yarn android:dev",
    "android:sb": "LOAD_STORYBOOK=true yarn android:dev",
    "ios:sb": "LOAD_STORYBOOK=true yarn ios:dev",
    "build-scripts": "node build-scripts.js",
    "clean": "rm -rf android ios && EXPO_NO_GIT_STATUS=1 yarn expo prebuild --clean --no-install && yarn react-native clean-project-auto && yarn && yarn pod-install",
    "migration:generate": "yarn build-scripts && node built-scripts/generate-new-migration.js",
    "update-schema-snapshot": "yarn build-scripts && node built-scripts/update-schema-snapshot.js",
    "check-realm-schema": "yarn build-scripts && node built-scripts/check-realm-schema.js",
    "bundle:ios": "react-native bundle --platform ios --dev false --entry-file index.js --bundle-output main.bundle.js",
    "bundle:android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output main.bundle.js",
    "create-android-emulator": "make create-emulator",
    "test": "NODE_ENV=test jest",
    "test:debug": "LOG_LEVEL=debug yarn test",
    "test:silent": "JEST_SILENT_REPORTER_DOTS=true yarn test --reporters=jest-silent-reporter",
    "test:ci": "yarn test --runInBand --coverage=true --workerIdleMemoryLimit=512MB",
    "lint": "eslint --cache --cache-strategy content .",
    "lint:ci": "eslint . --cache --cache-strategy content --cache-location .eslintcache",
    "storybook-generate": "sb-rn-get-stories",
    "uninstall-ios-sim-app": "xcrun simctl uninstall booted com.origamirisk.flex.dev",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "build:emm-plugin": "yarn tsc --build expo-plugins/emm-plugin",
    "build:android-manifest-application-plugin": "yarn tsc --build expo-plugins/with-android-manifest-application-attributes",
    "build:expo-plugins": "yarn build:emm-plugin && yarn build:android-manifest-application-plugin"
  },
